<form action='/songs' method="post">
  Name:
  <input id="song_name" type="text" name="song[name]"><br>
  Artist:
  <input id="song_artist" type="text" name="song[artist_id]"><br>
  Genre:
  <input id="song_genre" type="text" name="song[genre_id]"><br>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  Submit
  <input type="submit" name="Create song" id="Create song" >
</form>
